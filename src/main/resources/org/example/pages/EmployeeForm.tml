<html t:type="layout" title="Form | olderv5.6"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

<h2>Employee Form</h2>

<!--<t:beaneditform object="employee"></t:beaneditform>-->

<t:form t:id="employeeForm" t:type="form" >
    <div>
        <label for="meno">First Name:</label>
        <input id="meno" t:type="TextField" t:id="meno" t:value = "meno"/>
    </div>
    <div>
        <label for="priezvisko">Last Name:</label>
        <input id="priezvisko" t:type="TextField" t:id="priezvisko" t:value = "priezvisko" />
    </div>
    <div>
        <label for="titul">Title:</label>
        <t:select id="titul" t:id="titul" t:value="selectedTitle" t:model="titleOptions" />
    </div>
    <div>
        <label for="pohlavie">Gender:</label>
        <t:select id="pohlavie" t:id="pohlavie" t:value="selectedPohlavie" t:model="pohlavieOptions" />
    </div>
    <div>
        <label for="datumNarodenia">Birth Date:</label>
        <input id="datumNarodenia" t:type="DateField" t:id="datumNarodenia" />
    </div>
    <div>
        <input type="submit" value="Submit" />
        <t:if test="employee">
            <t:eventlink t:event="deleteEmployee" t:context="employee">Delete</t:eventlink>
        </t:if>
    </div>
</t:form>

</html>
